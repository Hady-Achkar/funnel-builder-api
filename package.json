{
  "name": "express-typescript-prisma-app",
  "version": "1.0.0",
  "description": "Express TypeScript app with Prisma and Docker",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "test": "prisma generate && vitest",
    "test:watch": "prisma generate && vitest --watch",
    "test:coverage": "prisma generate && vitest --coverage",
    "test:ui": "prisma generate && vitest --ui",
    "prepare": "echo 'Husky disabled - run: chmod +x .husky/* to enable'"
  },
  "dependencies": {
    "@azure/functions": "^4.7.2-preview",
    "@azure/storage-blob": "^12.28.0",
    "@prisma/client": "^6.13.0",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "prisma": "^5.7.1",
    "psl": "^1.9.0",
    "redis": "^4.6.12",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/psl": "^1.1.3",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prisma": "^6.13.0",
    "redis-memory-server": "^0.10.0",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {},
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "docker"
  ],
  "author": "",
  "license": "ISC"
}
