{
  "name": "express-typescript-prisma-app",
  "version": "1.0.0",
  "description": "Express TypeScript app with Prisma and Docker",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "express": "^4.18.2",
    "@prisma/client": "^5.7.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "axios": "^1.6.2",
    "psl": "^1.9.0",
    "redis": "^4.6.12",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/psl": "^1.1.3",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "prisma": "^5.7.1",
    "vitest": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@types/vitest": "^1.0.4",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "redis-memory-server": "^0.10.0"
  },
  "keywords": ["express", "typescript", "prisma", "docker"],
  "author": "",
  "license": "ISC"
}
