generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                         Int                  @id @default(autoincrement())
  email                      String               @unique
  username                   String               @unique
  firstName                  String
  lastName                   String
  password                   String
  verified                   Boolean              @default(false)
  verificationToken          String?              @unique
  verificationTokenExpiresAt DateTime?
  passwordResetToken         String?              @unique
  passwordResetExpiresAt     DateTime?
  isAdmin                    Boolean              @default(false)
  plan                       UserPlan             @default(NO_PLAN)
  registrationSource         RegistrationSource?  @default(DIRECT)
  registrationToken          String?              // Stores affiliateToken or workspaceInvitationToken used during registration
  referralLinkUsedId         Int?
  balance                    Float                @default(0)
  pendingBalance             Float                @default(0)
  avatar                     String?
  createdAt                  DateTime             @default(now())
  updatedAt                  DateTime             @updatedAt
  trialStartDate             DateTime?
  trialEndDate               DateTime?
  partnerLevel               Int                  @default(1)
  totalSales                 Int                  @default(0)
  commissionPercentage       Float                @default(5)
  addOns                     AddOn[]              @relation("UserAddOns")
  affiliateLinks             AffiliateLink[]      @relation("AffiliateOwner")
  balanceTransactions        BalanceTransaction[]
  createdDomains             Domain[]             @relation("DomainCreator")
  createdFunnels             Funnel[]             @relation("FunnelCreator")
  imageFolders               ImageFolder[]
  payments                   Payment[]            @relation("UserPayments")
  payouts                    Payout[]
  subscriptions              Subscription[]       @relation("UserSubscriptions")
  templates                  Template[]
  workspaceSales             WorkspaceClone[]     @relation("WorkspaceSales")
  workspacePurchases         WorkspaceClone[]     @relation("WorkspacePurchases")
  referralLinkUsed           AffiliateLink?       @relation("ReferralUsers", fields: [referralLinkUsedId], references: [id])
  sentInvitations            WorkspaceMember[]    @relation("InvitationSender")
  workspaceMembers           WorkspaceMember[]
  ownedWorkspaces            Workspace[]          @relation("WorkspaceOwner")

  @@map("users")
}

model AffiliateLink {
  id              Int        @id @default(autoincrement())
  name            String
  token           String     @unique
  itemType        UserPlan
  userId          Int
  workspaceId     Int
  clickCount      Int        @default(0)
  totalAmount     Float      @default(0)
  settings        Json?      @default("{}")
  createdAt       DateTime   @default(now())
  updatedAt       DateTime   @updatedAt
  user            User       @relation("AffiliateOwner", fields: [userId], references: [id], onDelete: Cascade)
  workspace       Workspace  @relation(fields: [workspaceId], references: [id], onDelete: Cascade)
  payments        Payment[]
  subscribedUsers User[]     @relation("ReferralUsers")

  @@index([token])
  @@index([userId])
  @@index([workspaceId])
  @@index([itemType])
  @@map("affiliate_links")
}

model Payment {
  id                    Int                @id @default(autoincrement())
  transactionId         String             @unique
  amount                Float?
  currency              String
  status                String
  itemType              UserPlan?
  paymentType           PaymentType        @default(PLAN_PURCHASE)
  addOnId               Int?
  addOnType             AddOnType?
  addOnQuantity         Int?
  buyerId               Int?
  affiliateLinkId       Int?
  affiliatePaid         Boolean            @default(false)
  commissionAmount      Float?
  commissionStatus      CommissionStatus?
  commissionHeldUntil   DateTime?
  commissionReleasedAt  DateTime?
  workspaceId           Int?
  rawData               Json?              @default("{}")
  createdAt             DateTime           @default(now())
  updatedAt             DateTime           @updatedAt
  addOn                 AddOn?             @relation(fields: [addOnId], references: [id])
  affiliateLink         AffiliateLink?     @relation(fields: [affiliateLinkId], references: [id])
  buyer                 User?              @relation("UserPayments", fields: [buyerId], references: [id], onDelete: SetNull)
  workspace             Workspace?         @relation(fields: [workspaceId], references: [id])
  workspaceClone        WorkspaceClone?

  @@index([buyerId])
  @@index([affiliateLinkId])
  @@index([workspaceId])
  @@index([status])
  @@index([transactionId])
  @@index([commissionStatus])
  @@map("payments")
}

model Subscription {
  id               Int                    @id @default(autoincrement())
  subscriptionId   String                 @unique
  subscriberId     String?
  startsAt         DateTime
  endsAt           DateTime
  status           SubscriptionStatus     @default(ACTIVE)
  userId           Int?
  intervalUnit     IntervalUnit
  intervalCount    Int                    @default(1)
  rawData          Json?                  @default("{}")
  itemType         SubscriptionItemType
  subscriptionType UserPlan?
  addonType        AddOnType?
  createdAt        DateTime               @default(now())
  updatedAt        DateTime               @updatedAt
  user             User?                  @relation("UserSubscriptions", fields: [userId], references: [id], onDelete: SetNull)

  @@index([userId])
  @@index([status])
  @@index([subscriptionId])
  @@index([itemType])
  @@map("subscriptions")
}

model Workspace {
  id                Int                         @id @default(autoincrement())
  name              String
  slug              String                      @unique
  ownerId           Int
  description       String?
  settings          Json?                       @default("{}")
  createdAt         DateTime                    @default(now())
  updatedAt         DateTime                    @updatedAt
  image             String?
  status            WorkspaceStatus             @default(ACTIVE)
  planType          UserPlan                    @default(FREE)
  addOns            AddOn[]                     @relation("WorkspaceAddOns")
  affiliateLinks    AffiliateLink[]
  domains           Domain[]
  funnels           Funnel[]
  members           WorkspaceMember[]
  payments          Payment[]
  rolePermTemplates WorkspaceRolePermTemplate[]
  sourceClones      WorkspaceClone[]            @relation("SourceWorkspace")
  clonedFrom        WorkspaceClone?             @relation("ClonedWorkspace")
  owner             User                        @relation("WorkspaceOwner", fields: [ownerId], references: [id], onDelete: Cascade)

  @@map("workspaces")
}

model WorkspaceMember {
  id          Int                   @id @default(autoincrement())
  userId      Int?
  email       String?
  workspaceId Int
  role        WorkspaceRole
  permissions WorkspacePermission[]
  status      MembershipStatus      @default(ACTIVE)
  invitedBy   Int?
  invitedAt   DateTime              @default(now())
  joinedAt    DateTime?
  updatedAt   DateTime              @updatedAt
  inviter     User?                 @relation("InvitationSender", fields: [invitedBy], references: [id])
  user        User?                 @relation(fields: [userId], references: [id], onDelete: Cascade)
  workspace   Workspace             @relation(fields: [workspaceId], references: [id], onDelete: Cascade)

  @@unique([userId, workspaceId])
  @@index([workspaceId])
  @@index([userId])
  @@index([email])
  @@index([status])
  @@map("workspace_members")
}

model WorkspaceRolePermTemplate {
  id          Int                   @id @default(autoincrement())
  workspaceId Int
  role        WorkspaceRole
  permissions WorkspacePermission[]
  createdAt   DateTime              @default(now())
  updatedAt   DateTime              @updatedAt
  workspace   Workspace             @relation(fields: [workspaceId], references: [id], onDelete: Cascade)

  @@unique([workspaceId, role])
  @@index([workspaceId])
  @@map("workspace_role_perm_templates")
}

model Funnel {
  id                Int             @id @default(autoincrement())
  name              String
  slug              String
  status            FunnelStatus    @default(DRAFT)
  workspaceId       Int
  createdBy         Int
  activeThemeId     Int?
  createdAt         DateTime        @default(now())
  updatedAt         DateTime        @updatedAt
  domainConnections FunnelDomain[]
  settings          FunnelSettings?
  activeTheme       Theme?          @relation("ActiveTheme", fields: [activeThemeId], references: [id])
  creator           User            @relation("FunnelCreator", fields: [createdBy], references: [id])
  workspace         Workspace       @relation(fields: [workspaceId], references: [id], onDelete: Cascade)
  insights          Insight[]
  pages             Page[]
  customTheme       Theme?          @relation("CustomTheme")

  @@unique([workspaceId, name])
  @@unique([workspaceId, slug])
  @@index([workspaceId])
  @@index([createdBy])
  @@index([status])
  @@index([activeThemeId])
  @@map("funnels")
}

model Domain {
  id                    Int            @id @default(autoincrement())
  hostname              String         @unique
  type                  DomainType
  status                DomainStatus   @default(PENDING)
  sslStatus             SslStatus      @default(PENDING)
  workspaceId           Int?
  createdBy             Int

  // Azure Front Door fields (all domains use Azure now)
  azureCustomDomainName String?        @unique // Sanitized name for Azure (e.g., 'example-com' or 'mysite-digitalsite-io')
  azureDomainStatus     String?        // Azure validation state (Pending, Approved, Rejected)
  azureCertStatus       String?        // Azure certificate status (Pending, Issued, Expired)

  // Domain verification and DNS
  verificationToken     String?        // Validation token for domain ownership
  ownershipVerification Json?          // Ownership verification details
  dnsInstructions       Json?          // DNS setup instructions for user
  lastVerifiedAt        DateTime?      // Last time domain was verified
  expiresAt             DateTime?      // Domain expiration (if applicable)
  notes                 String?        // Additional notes
  createdAt             DateTime       @default(now())
  updatedAt             DateTime       @updatedAt
  creator               User           @relation("DomainCreator", fields: [createdBy], references: [id])
  workspace             Workspace?     @relation(fields: [workspaceId], references: [id], onDelete: Cascade)
  funnelConnections     FunnelDomain[]

  @@index([workspaceId])
  @@index([createdBy])
  @@index([status])
  @@index([type])
  @@index([azureCustomDomainName])
  @@map("domains")
}

model FunnelDomain {
  id        Int      @id @default(autoincrement())
  funnelId  Int
  domainId  Int
  isActive  Boolean  @default(true)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  domain    Domain   @relation(fields: [domainId], references: [id], onDelete: Cascade)
  funnel    Funnel   @relation(fields: [funnelId], references: [id], onDelete: Cascade)

  @@unique([funnelId, domainId])
  @@index([funnelId])
  @@index([domainId])
  @@map("funnel_domains")
}

model Page {
  id             Int      @id @default(autoincrement())
  name           String
  content        String?
  order          Int
  type           PageType @default(PAGE)
  linkingId      String?
  seoTitle       String?
  seoDescription String?
  seoKeywords    String?
  visits         Int      @default(0)
  funnelId       Int
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  funnel         Funnel   @relation(fields: [funnelId], references: [id], onDelete: Cascade)

  @@unique([funnelId, linkingId])
  @@map("pages")
}

model Session {
  id                 String              @id @default(cuid())
  sessionId          String              @unique
  funnelId           Int
  visitedPages       Int[]               @default([])
  interactions       Json                @default("{}")
  createdAt          DateTime            @default(now())
  updatedAt          DateTime            @updatedAt
  formSubmissions    FormSubmission[]
  insightSubmissions InsightSubmission[]

  @@index([sessionId])
  @@index([funnelId])
  @@index([createdAt])
  @@map("sessions")
}

model Theme {
  id              Int          @id @default(autoincrement())
  name            String       @default("Default Theme")
  backgroundColor String       @default("#0e1e12")
  textColor       String       @default("#d4ecd0")
  buttonColor     String       @default("#387e3d")
  buttonTextColor String       @default("#e8f5e9")
  borderColor     String       @default("#214228")
  optionColor     String       @default("#16331b")
  fontFamily      String       @default("Inter, sans-serif")
  borderRadius    BorderRadius @default(SOFT)
  type            ThemeType    @default(CUSTOM)
  funnelId        Int?         @unique
  createdAt       DateTime     @default(now())
  updatedAt       DateTime     @updatedAt
  funnels         Funnel[]     @relation("ActiveTheme")
  funnel          Funnel?      @relation("CustomTheme", fields: [funnelId], references: [id], onDelete: Cascade)

  @@index([funnelId])
  @@index([type])
  @@map("themes")
}

model Insight {
  id          Int                 @id @default(autoincrement())
  type        InsightType
  name        String
  description String?
  content     Json
  settings    Json?               @default("{}")
  funnelId    Int
  createdAt   DateTime            @default(now())
  updatedAt   DateTime            @updatedAt
  submissions InsightSubmission[]
  funnel      Funnel              @relation(fields: [funnelId], references: [id], onDelete: Cascade)

  @@index([funnelId])
  @@index([type])
  @@map("insights")
}

model InsightSubmission {
  id          Int       @id @default(autoincrement())
  insightId   Int
  sessionId   String
  answers     Json?
  completedAt DateTime?
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
  insight     Insight   @relation(fields: [insightId], references: [id], onDelete: Cascade)
  session     Session   @relation(fields: [sessionId], references: [sessionId], onDelete: Cascade)

  @@unique([insightId, sessionId])
  @@index([sessionId])
  @@map("insight_submissions")
}

model TemplateCategory {
  id          Int        @id @default(autoincrement())
  name        String     @unique
  slug        String     @unique
  description String?
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
  templates   Template[]

  @@index([slug])
  @@map("template_categories")
}

model Template {
  id              Int              @id @default(autoincrement())
  name            String
  slug            String           @unique
  description     String?
  categoryId      Int
  tags            String[]
  usageCount      Int              @default(0)
  isActive        Boolean          @default(true)
  isPublic        Boolean          @default(true)
  createdByUserId Int
  createdAt       DateTime         @default(now())
  updatedAt       DateTime         @updatedAt
  previewImages   TemplateImage[]
  pages           TemplatePage[]
  category        TemplateCategory @relation(fields: [categoryId], references: [id])
  createdBy       User             @relation(fields: [createdByUserId], references: [id])

  @@index([slug])
  @@index([categoryId])
  @@index([isActive, isPublic])
  @@index([createdByUserId])
  @@map("templates")
}

model TemplateImage {
  id         Int               @id @default(autoincrement())
  templateId Int
  imageUrl   String
  imageType  TemplateImageType @default(PREVIEW)
  order      Int               @default(0)
  caption    String?
  createdAt  DateTime          @default(now())
  updatedAt  DateTime          @updatedAt
  template   Template          @relation(fields: [templateId], references: [id], onDelete: Cascade)

  @@index([templateId])
  @@map("template_images")
}

model TemplatePage {
  id             Int      @id @default(autoincrement())
  templateId     Int
  name           String
  content        String?
  order          Int
  type           PageType @default(PAGE)
  settings       Json?
  linkingId      String?  @unique
  seoTitle       String?
  seoDescription String?
  seoKeywords    String?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  template       Template @relation(fields: [templateId], references: [id], onDelete: Cascade)

  @@index([templateId])
  @@map("template_pages")
}

model Image {
  id        Int          @id @default(autoincrement())
  name      String?
  url       String
  altText   String?
  size      Int?
  folderId  Int?
  createdAt DateTime     @default(now())
  updatedAt DateTime     @updatedAt
  folder    ImageFolder? @relation("ImageFolderImages", fields: [folderId], references: [id], onDelete: Cascade)

  @@map("images")
}

model ImageFolder {
  id        Int      @id @default(autoincrement())
  name      String
  userId    Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  user      User     @relation(fields: [userId], references: [id], onDelete: Cascade)
  images    Image[]  @relation("ImageFolderImages")

  @@unique([userId, name])
  @@index([userId])
  @@map("image_folders")
}

model Form {
  id                  Int              @id @default(autoincrement())
  name                String
  description         String?
  formContent         Json
  isActive            Boolean          @default(true)
  funnelId            Int?
  webhookUrl          String?
  webhookEnabled      Boolean          @default(false)
  webhookHeaders      Json?            @default("{}")
  webhookSecret       String?
  webhookSuccessCount Int              @default(0)
  webhookFailureCount Int              @default(0)
  lastWebhookAt       DateTime?
  createdAt           DateTime         @default(now())
  updatedAt           DateTime         @updatedAt
  submissions         FormSubmission[]

  @@index([isActive])
  @@index([funnelId])
  @@index([webhookEnabled])
  @@map("forms")
}

model FormSubmission {
  id            Int       @id @default(autoincrement())
  formId        Int
  sessionId     String
  submittedData Json?
  isCompleted   Boolean   @default(false)
  completedAt   DateTime?
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
  form          Form      @relation(fields: [formId], references: [id], onDelete: Cascade)
  session       Session   @relation(fields: [sessionId], references: [sessionId], onDelete: Cascade)

  @@unique([formId, sessionId])
  @@index([sessionId])
  @@index([isCompleted])
  @@map("form_submissions")
}

model FunnelSettings {
  id                    Int      @id @default(autoincrement())
  funnelId              Int      @unique
  defaultSeoTitle       String?
  defaultSeoDescription String?
  defaultSeoKeywords    String?
  favicon               String?
  ogImage               String?
  googleAnalyticsId     String?
  facebookPixelId       String?
  customTrackingScripts Json?    @default("[]")
  enableCookieConsent   Boolean  @default(false)
  cookieConsentText     String?
  privacyPolicyUrl      String?
  termsOfServiceUrl     String?
  language              String?  @default("en")
  timezone              String?  @default("UTC")
  dateFormat            String?  @default("DD.MM.YYYY")
  isPasswordProtected   Boolean  @default(false)
  passwordHash          String?
  createdAt             DateTime @default(now())
  updatedAt             DateTime @updatedAt
  funnel                Funnel   @relation(fields: [funnelId], references: [id], onDelete: Cascade)

  @@index([funnelId])
  @@map("funnel_settings")
}

model AddOn {
  id                  Int          @id @default(autoincrement())
  userId              Int
  workspaceId         Int?
  type                AddOnType
  quantity            Int          @default(1)
  pricePerUnit        Float
  status              AddOnStatus  @default(ACTIVE)
  billingCycle        IntervalUnit @default(MONTH)
  startDate           DateTime     @default(now())
  endDate             DateTime?
  expirationReminders Json?        @default("{}") // Track sent reminder emails and resource processing: {day7: true, day3: true, day1: true, resourcesProcessed: true}
  createdAt           DateTime     @default(now())
  updatedAt           DateTime     @updatedAt
  user                User         @relation("UserAddOns", fields: [userId], references: [id], onDelete: Cascade)
  workspace           Workspace?   @relation("WorkspaceAddOns", fields: [workspaceId], references: [id], onDelete: Cascade)
  payments            Payment[]

  @@index([userId])
  @@index([workspaceId])
  @@index([status])
  @@map("addons")
}

model BalanceTransaction {
  id              Int                    @id @default(autoincrement())
  userId          Int
  type            BalanceTransactionType
  amount          Float
  balanceBefore   Float
  balanceAfter    Float
  referenceType   String?
  referenceId     Int?
  notes           String?
  releasedAt      DateTime?
  createdAt       DateTime               @default(now())
  payoutId        Int?                   @unique
  user            User                   @relation(fields: [userId], references: [id], onDelete: Cascade)
  payout          Payout?                @relation(fields: [payoutId], references: [id])

  @@index([userId])
  @@index([referenceType, referenceId])
  @@index([createdAt])
  @@map("balance_transactions")
}

model WorkspaceClone {
  id                Int       @id @default(autoincrement())
  sourceWorkspaceId Int
  clonedWorkspaceId Int       @unique
  sellerId          Int?
  buyerId           Int?
  paymentId         Int       @unique
  clonedAt          DateTime  @default(now())
  sourceWorkspace   Workspace @relation("SourceWorkspace", fields: [sourceWorkspaceId], references: [id])
  clonedWorkspace   Workspace @relation("ClonedWorkspace", fields: [clonedWorkspaceId], references: [id])
  seller            User?     @relation("WorkspaceSales", fields: [sellerId], references: [id], onDelete: SetNull)
  buyer             User?     @relation("WorkspacePurchases", fields: [buyerId], references: [id], onDelete: SetNull)
  payment           Payment   @relation(fields: [paymentId], references: [id])

  @@index([sourceWorkspaceId])
  @@index([clonedWorkspaceId])
  @@index([sellerId])
  @@index([buyerId])
  @@index([paymentId])
  @@map("workspace_clones")
}

model Payout {
  id                 Int                 @id @default(autoincrement())
  userId             Int
  amount             Float               // Requested amount
  fees               Float               @default(0) // Platform fees
  netAmount          Float               // Amount after fees (amount - fees)
  currency           String              @default("USD")
  status             PayoutStatus
  method             PayoutMethod        // UAE_BANK, INTERNATIONAL_BANK, USDT

  // Payment destination details
  accountHolderName  String?             // Bank account holder name
  bankName           String?             // Bank name
  accountNumber      String?             // Bank account/IBAN
  swiftCode          String?             // SWIFT/BIC for international
  bankAddress        String?             // Bank address for international transfers
  usdtWalletAddress  String?             // USDT wallet address
  usdtNetwork        String?             // TRC20, ERC20, etc.

  // Supporting documents
  documentUrl        String?             // Uploaded file (invoice, ID, etc.)
  documentType       String?             // Type of document uploaded

  // Processing details
  processedAt        DateTime?
  failedAt           DateTime?
  failureReason      String?
  transactionId      String?      @unique // External payment ID
  transactionProof   String?             // URL to transaction proof/receipt

  // Notes
  userNotes          String?             // User's notes/description
  adminNotes         String?             // Internal admin notes

  // Admin tracking
  adminHistory       Json?               // History of admin actions with codes and status changes

  createdAt          DateTime     @default(now())
  updatedAt          DateTime     @updatedAt

  // Relations
  user               User         @relation(fields: [userId], references: [id])
  balanceTransaction BalanceTransaction?

  @@index([userId])
  @@index([status])
  @@index([method])
  @@index([createdAt])
  @@map("payouts")
}

enum UserPlan {
  NO_PLAN
  WORKSPACE_MEMBER
  BUSINESS
  AGENCY
  FREE
}

enum SubscriptionStatus {
  ACTIVE
  CANCELLED
  EXPIRED
  PENDING
}

enum SubscriptionItemType {
  PLAN
  ADDON
}

enum IntervalUnit {
  WEEK
  MONTH
  YEAR
}

enum PaymentType {
  PLAN_PURCHASE
  ADDON_PURCHASE
  ADDON_RENEWAL
}

enum WorkspaceRole {
  OWNER
  ADMIN
  EDITOR
  VIEWER
}

enum WorkspacePermission {
  MANAGE_WORKSPACE
  MANAGE_MEMBERS
  CREATE_FUNNELS
  EDIT_FUNNELS
  EDIT_PAGES
  DELETE_FUNNELS
  VIEW_ANALYTICS
  MANAGE_DOMAINS
  CREATE_DOMAINS
  DELETE_DOMAINS
  CONNECT_DOMAINS
}

enum MembershipStatus {
  PENDING
  ACTIVE
  REJECTED
}

enum WorkspaceStatus {
  ACTIVE
  DRAFT
  ARCHIVED
  SUSPENDED
}

enum FunnelStatus {
  DRAFT
  LIVE
  ARCHIVED
  SHARED
}

enum PageType {
  PAGE
  RESULT
}

enum DomainType {
  CUSTOM_DOMAIN
  SUBDOMAIN
}

enum DomainStatus {
  PENDING
  VERIFIED
  ACTIVE
  FAILED
  SUSPENDED
}

enum SslStatus {
  PENDING
  ACTIVE
  ERROR
  EXPIRED
}

enum BorderRadius {
  NONE
  SOFT
  ROUNDED
}

enum ThemeType {
  GLOBAL
  CUSTOM
}

enum InsightType {
  QUIZ
  SINGLE_CHOICE
  MULTIPLE_CHOICE
}

enum TemplateImageType {
  THUMBNAIL
  PREVIEW
}

enum AddOnType {
  EXTRA_ADMIN
  EXTRA_FUNNEL
  EXTRA_PAGE
  EXTRA_SUBDOMAIN
  EXTRA_CUSTOM_DOMAIN
  EXTRA_WORKSPACE
}

enum AddOnStatus {
  ACTIVE
  CANCELLED
  EXPIRED
  PENDING
}

enum BalanceTransactionType {
  COMMISSION
  WITHDRAWAL
  WITHDRAWAL_REVERSAL
  ADJUSTMENT
  REFUND
  COMMISSION_HOLD
  COMMISSION_RELEASE
  COMMISSION_REVERSED
}

enum CommissionStatus {
  PENDING
  RELEASED
  REVERSED
}

enum RegistrationSource {
  DIRECT
  WORKSPACE_INVITE
  AFFILIATE
}

enum PayoutStatus {
  PENDING
  PROCESSING
  COMPLETED
  FAILED
  CANCELLED
  ON_HOLD
}

enum PayoutMethod {
  UAE_BANK
  INTERNATIONAL_BANK
  USDT
}
